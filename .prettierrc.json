{
  "trailingComma": "es5",
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 100,
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "useTabs": false,
  "importOrder": ["^react", "^[^.]", "", "^.*(?<!\\.(mp3|scss))$", "", "\\.mp3$", "", "\\.scss$"],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "importOrderMergeDuplicateImports": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
